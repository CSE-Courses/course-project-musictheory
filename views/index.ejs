        <input class="form-control mr-sm-2" type="text" placeholder="Search" aria-label="Search">
        <a href="/search" class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
      </form>

      <a class="navbar-brand" href="/signin">Sign In</a>

